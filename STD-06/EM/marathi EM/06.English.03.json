    { "प्रश्न_आयडी": 24, "प्रश्न": "आजोबा एकटे सामान का आणत होते?", "उत्तर": "घरातले सगळे लोक कामाला गेले होते, म्हणून आजोबांना एकट्याला सामान आणावे लागले." },
    { "प्रश्न_आयडी": 25, "प्रश्न": "बाबांनी आजोबांना काय मदत केली?", "उत्तर": "बाबांनी आजोबांचे जड सामान उचलले आणि त्यांना गाडीतून घरापर्यंत सोडले." },
    { "प्रश्न_आयडी": 26, "प्रश्न": "मुले भिंगाच्या साह्याने काय करतात?", "उत्तर": "मुले भिंगातून उन्हाची किरणे कागदावर पाडून कागद पेटवतात." },
    { "प्रश्न_आयडी": 27, "प्रश्न": "कागद पेटणे ही जादू आहे का?", "उत्तर": "नाही, ही जादू नसून ते विज्ञानाचे तत्व आहे." },
    { "प्रश्न_आयडी": 28, "प्रश्न": "कागद का पेटतो?", "उत्तर": "सूर्याची उष्णता भिंगातून एका जागी एकवटल्यामुळे कागद पेटतो." },
    { "प्रश्न_आयडी": 29, "प्रश्न": "सौरऊर्जा म्हणजे काय?", "उत्तर": "सूर्यापासून मिळणारी आणि साठवलेली शक्ती म्हणजे सौरऊर्जा होय." },
    { "प्रश्न_आयडी": 30, "प्रश्न": "सौरऊर्जेवर कोणती उपकरणे चालतात?", "उत्तर": "सौरऊर्जेवर रस्त्यावरचे दिवे, रेडिओ, संगणक इत्यादी चालतात." },
    { "प्रश्न_आयडी": 31, "प्रश्न": "सूर्यापासून पृथ्वीवर काय येते?", "उत्तर": "सूर्यापासून पृथ्वीवर प्रचंड उष्णता आणि प्रकाश येतो." },
    { "प्रश्न_आयडी": 32, "प्रश्न": "आपण सूर्याची कोणती गोष्ट वाया घालवत आहोत?", "उत्तर": "आपण सूर्यापासून मिळणारी प्रचंड उष्णता वाया घालवत आहोत." },
    { "प्रश्न_आयडी": 33, "प्रश्न": "निसर्गाचा समतोल का बिघडत आहे?", "उत्तर": "वृक्षतोड आणि प्रदूषणामुळे निसर्गाचा समतोल बिघडत आहे." },
    { "प्रश्न_आयडी": 34, "प्रश्न": "शुद्ध हवेसाठी काय आवश्यक आहे?", "उत्तर": "शुद्ध हवेसाठी भरपूर झाडे लावणे आवश्यक आहे." },
    { "प्रश्न_आयडी": 35, "प्रश्न": "पाणी प्रदूषण कशामुळे होते?", "उत्तर": "नदीत सांडपाणी आणि कारखान्यातील रसायने सोडल्यामुळे पाणी प्रदूषण होते." },
    { "प्रश्न_आयडी": 36, "प्रश्न": "कवितेतील 'मानव' कोणाला उद्देशून आहे?", "उत्तर": "कवितेतील 'मानव' हा सर्व मानवी समाजाला उद्देशून आहे." },
    { "प्रश्न_आयडी": 37, "प्रश्न": "धरणी स्वच्छ ठेवण्यासाठी आपण काय करू शकतो?", "उत्तर": "कचरा न टाकणे आणि प्रदूषण कमी करणे यातून आपण धरणी स्वच्छ ठेवू शकतो." },
    { "प्रश्न_आयडी": 38, "प्रश्न": "पावसाचे पाणी साठवणे का गरजेचे आहे?", "उत्तर": "भविष्यातील पाण्याची टंचाई दूर करण्यासाठी पावसाचे पाणी साठवणे गरजेचे आहे." },
    { "प्रश्न_आयडी": 39, "प्रश्न": "विकासाची संकल्पना काय असावी?", "उत्तर": "निसर्गाचे नुकसान न करता प्रगती करणे ही खरी विकासाची संकल्पना असावी." },
    { "प्रश्न_आयडी": 40, "प्रश्न": "'पर्यावरणाचे भान' म्हणजे नक्की काय?", "उत्तर": "आपल्या कृतीचा पर्यावरणावर काय परिणाम होईल याची जाणीव असणे." },
    { "प्रश्न_आयडी": 41, "प्रश्न": "कारखान्यांचे पाणी नदीत सोडल्याने काय नुकसान होते?", "उत्तर": "नदीतील पाणी विषारी होते आणि जलचर प्राणी मरतात." },
    { "प्रश्न_आयडी": 42, "प्रश्न": "आपण नियम पाळण्याबाबत इतरांकडून काय अपेक्षा करतो?", "उत्तर": "आपण अपेक्षा करतो की नियमांचे पालन इतरांनी करावे." },
    { "प्रश्न_आयडी": 43, "प्रश्न": "आजोबा सावलीत का थांबले?", "उत्तर": "बाबांनी त्यांना सांगितले की मी मीनाला सोडून लगेच येतो, तोपर्यंत तुम्ही सावलीत थांबा." },
    { "प्रश्न_आयडी": 44, "प्रश्न": "सौरशक्तीचा वापर केल्याने काय फायदा होईल?", "उत्तर": "सौरशक्तीमुळे प्रदूषणात घट होईल आणि इंधनाची बचत होईल." },
    { "प्रश्न_आयडी": 45, "प्रश्न": "भिंगातून कागद पेटवताना कोणती किरणे वापरली जातात?", "उत्तर": "सूर्याची प्रखर किरणे वापरली जातात." },
    { "प्रश्न_आयडी": 46, "प्रश्न": "जगातील प्रत्येक व्यक्तीने काय ठरवले पाहिजे?", "उत्तर": "प्रत्येक व्यक्तीने निसर्गाचे रक्षण करण्याचे ठरवले पाहिजे." },
    { "प्रश्न_आयडी": 47, "प्रश्न": "निसर्ग कोणाला कळायला हवा?", "उत्तर": "निसर्ग मानवाला कळायला हवा." },
    { "प्रश्न_आयडी": 48, "प्रश्न": "पावसाची सर आल्यावर कवीला आनंद का होत नाही?", "उत्तर": "पाणी प्रदूषित असल्यामुळे आणि भविष्यातील टंचाईमुळे कवीला पूर्ण आनंद होत नाही." },
    { "प्रश्न_आयडी": 49, "प्रश्न": "विज्ञानाचा उपयोग पर्यावरणासाठी कसा करावा?", "उत्तर": "सौरऊर्जेसारख्या प्रदूषणमुक्त ऊर्जेचा वापर करून विज्ञानाचा उपयोग पर्यावरणासाठी करावा." },
    { "प्रश्न_आयडी": 50, "प्रश्न": "कवितेचे मुख्य शीर्षक काय सुचवते?", "उत्तर": "माणसाने निसर्गाचे महत्त्व समजून घ्यावे हे कवितेचे शीर्षक सुचवते." },
    { "प्रश्न_आयडी": 51, "प्रश्न": "शुद्ध पाणी ही मानवाची काय आहे?", "उत्तर": "शुद्ध पाणी ही मानवाची मूलभूत गरज आहे." },
    { "प्रश्न_आयडी": 52, "प्रश्न": "रसायने पाण्यात सोडणे म्हणजे काय?", "उत्तर": "रसायने पाण्यात सोडणे म्हणजे निसर्गाचा विनाश करणे होय." },
    { "प्रश्न_आयडी": 53, "प्रश्न": "निसर्गाची काळजी घेतल्यास भविष्य कसे असेल?", "उत्तर": "निसर्गाची काळजी घेतल्यास भविष्य उज्ज्वल आणि आरोग्यदायी असेल." },
    { "प्रश्न_आयडी": 54, "प्रश्न": "आजोबांचे घर जवळ होते की लांब?", "उत्तर": "आजोबांना चालत जाणे कठीण होते, याचा अर्थ घर थोडे लांब किंवा पिशवी खूप जड होती." },
    { "प्रश्न_आयडी": 55, "प्रश्न": "सौरऊर्जेचा संचय कसा केला जातो?", "उत्तर": "सौर उपकरणांच्या साह्याने सूर्याची उष्णता साठवून तिचा संचय केला जातो." },
    { "प्रश्न_आयडी": 56, "प्रश्न": "कवितेत 'घाण' हा शब्द कोणत्या संदर्भात आला आहे?", "उत्तर": "नदी आणि परिसरात पसरलेल्या कचऱ्याच्या संदर्भात आला आहे." },
    { "प्रश्न_आयडी": 57, "प्रश्न": "'सताविते' या क्रियापदाचे मूळ रूप काय?", "उत्तर": "सतावणे." },
    { "प्रश्न_आयडी": 58, "प्रश्न": "माणसाला विकासाचा गर्व का आहे?", "उत्तर": "नवे कारखाने आणि तंत्रज्ञान शोधल्यामुळे माणसाला विकासाचा गर्व आहे." },
    { "प्रश्न_आयडी": 59, "प्रश्न": "कवीने मांडलेली समस्या जागतिक आहे का?", "उत्तर": "हो, पर्यावरण प्रदूषण ही एक जागतिक समस्या आहे." },
    { "प्रश्न_आयडी": 60, "प्रश्न": "स्वच्छता मोहिमेत मुलांचा सहभाग का हवा?", "उत्तर": "मुले देशाचे भविष्य आहेत, म्हणून त्यांनी स्वच्छतेची सवय लावून घेणे गरजेचे आहे." },
    { "प्रश्न_आयडी": 61, "प्रश्न": "नदीतील जलचरांवर प्रदूषणाचा काय परिणाम होतो?", "उत्तर": "प्रदूषणामुळे मासे आणि इतर जलचर मृत पावतात." },
    { "प्रश्न_आयडी": 62, "प्रश्न": "भिंगाच्या प्रयोगात 'उष्णता एकवटणे' म्हणजे काय?", "उत्तर": "विखुरलेली किरणे एका बिंदूवर केंद्रित करणे." },
    { "प्रश्न_आयडी": 63, "प्रश्न": "सूर्याची शक्ती कशी आहे?", "उत्तर": "सूर्याची शक्ती प्रचंड आणि कधीही न संपणारी आहे." },
    { "प्रश्न_आयडी": 64, "प्रश्न": "माणूस निसर्गाचा काय लागतो?", "उत्तर": "माणूस निसर्गाचा एक अविभाज्य भाग लागतो." },
    { "प्रश्न_आयडी": 65, "प्रश्न": "कवितेत 'पावसाची सर' कशाचे प्रतीक आहे?", "उत्तर": "ती निसर्गाच्या चक्राचे आणि जीवनाचे प्रतीक आहे." },
    { "प्रश्न_आयडी": 66, "प्रश्न": "वृद्ध माणसांची सेवा करणे हे काय आहे?", "उत्तर": "ती एक सामाजिक आणि नैतिक जबाबदारी आहे." },
    { "प्रश्न_आयडी": 67, "प्रश्न": "कवीने 'सोनियाचे दिस' कोणाला आणायचे आहेत?", "उत्तर": "येणाऱ्या पुढच्या पिढीला चांगले दिवस मिळावेत असे कवीला वाटते." },
    { "प्रश्न_आयडी": 68, "प्रश्न": "पाण्याचे नियोजन कसे करावे?", "उत्तर": "पाण्याचा काटकसरीने वापर करून आणि साठा करून नियोजन करावे." },
    { "प्रश्न_आयडी": 69, "प्रश्न": "विज्ञानामुळे काय सोपे झाले आहे?", "उत्तर": "विज्ञानामुळे कष्ट कमी झाले आहेत आणि सौरऊर्जेसारखे नवे मार्ग सापडले आहेत." },
    { "प्रश्न_आयडी": 70, "प्रश्न": "कवितेचा शेवट कोणत्या आशेने होतो?", "उत्तर": "मानवाला निसर्ग कळेल आणि चांगले दिवस येतील या आशेने कवितेचा शेवट होतो." }
  ]
}