    { "प्रश्न_आयडी": 16, "प्रश्न": "या कवितेचे कवी कोण आहेत?", "उत्तर": "या कवितेचे कवी राजन साटम आहेत." },
    { "प्रश्न_आयडी": 17, "प्रश्न": "चंद्रावर कवीला कोणते आश्चर्य जाणवले?", "उत्तर": "तिथे दिवस आणि रात्र नसून केवळ प्रकाश-अंधाराची रिमझिम होती, हे आश्चर्य कवीला जाणवले." },
    { "प्रश्न_आयडी": 18, "प्रश्न": "ससा-बिसा या शब्दाचा अर्थ काय?", "उत्तर": "ससा किंवा तसाच दुसरा कोणताही प्राणी असा त्याचा अर्थ होतो." },
    { "प्रश्न_आयडी": 19, "प्रश्न": "आर्मस्ट्राँग कोण होते?", "उत्तर": "नील आर्मस्ट्राँग हे चंद्रावर पाऊल ठेवणारे पहिले मानव (अंतराळवीर) होते." },
    { "प्रश्न_आयडी": 20, "प्रश्न": "कवितेत 'तरंगत तरंगत' हा शब्द का वापरला आहे?", "उत्तर": "चंद्रावरील कमी गुरुत्वाकर्षणामुळे माणूस तरंगतो, हे दर्शवण्यासाठी हा शब्द वापरला आहे." },
    { "प्रश्न_आयडी": 21, "प्रश्न": "कवी चंद्रावर धावले का?", "उत्तर": "नाही, कवी चंद्रावर तरंगत तरंगत हिंडले." },
    { "प्रश्न_आयडी": 22, "प्रश्न": "पौर्णिमेचे चांदणे पृथ्वीवरून कसे दिसते?", "उत्तर": "पौर्णिमेचे चांदणे पृथ्वीवरून दुधासारखे पांढरे दिसते." },
    { "प्रश्न_आयडी": 23, "प्रश्न": "चंद्रावर कवीला भीती वाटली का?", "उत्तर": "नाही, कवी तिथे आरामात हिंडत होते, त्यांना भीती वाटल्याचा उल्लेख नाही." },
    { "प्रश्न_आयडी": 24, "प्रश्न": "प्रकाश आणि अंधाराची रिमझिम म्हणजे काय?", "उत्तर": "स्पष्ट उजेड किंवा पूर्ण काळोख नसून धूसर प्रकाशाचे वातावरण असा त्याचा अर्थ होतो." },
    { "प्रश्न_आयडी": 25, "प्रश्न": "'स्वैर संचार' म्हणजे काय?", "उत्तर": "स्वतःच्या मर्जीने कोठेही मुक्तपणे फिरणे म्हणजे स्वैर संचार." },
    { "प्रश्न_आयडी": 26, "प्रश्न": "कवीला कापूस का व्हावेसे वाटले?", "उत्तर": "चंद्रावर स्वतःचे वजन खूप कमी जाणवत असल्यामुळे कवीला आपण कापूस झालो असे वाटले." },
    { "प्रश्न_आयडी": 27, "प्रश्न": "ठसा कोठे उमटला होता?", "उत्तर": "ठसा चंद्राच्या जमिनीवर (पृष्ठभागावर) उमटला होता." },
    { "प्रश्न_आयडी": 28, "प्रश्न": "चंद्रावर ससा असतो का?", "उत्तर": "नाही, चंद्रावर कोणताही ससा किंवा सजीव प्राणी नाही." },
    { "प्रश्न_आयडी": 29, "प्रश्न": "कवीने चंद्रावर काय पाहिले?", "उत्तर": "कवीने चंद्रावर आर्मस्ट्राँगचा ठसा आणि उगवलेली निळी पृथ्वी पाहिली." },
    { "प्रश्न_आयडी": 30, "प्रश्न": "पृथ्वीच्या चांदण्याला निळे का म्हटले आहे?", "उत्तर": "पृथ्वीवर पाण्याचे प्रमाण जास्त असल्याने ती अवकाशातून निळी दिसते, म्हणून तिच्या चांदण्याला निळे म्हटले आहे." },
    { "प्रश्न_आयडी": 31, "प्रश्न": "आरामात हिंडणे म्हणजे काय?", "उत्तर": "कोणतीही घाई न करता शांतपणे फिरणे." },
    { "प्रश्न_आयडी": 32, "प्रश्न": "चंद्रावर रात्र का नसते?", "उत्तर": "चंद्रावर पृथ्वीसारखा २४ तासांचा दिवस-रात्रीचा चक्र नसतो, तिथे एका दिवसाचा कालावधी खूप मोठा असतो." },
    { "प्रश्न_आयडी": 33, "प्रश्न": "कवी चंद्रावर विमानाने गेले का?", "उत्तर": "कवितेत कवी केवळ 'गेलो तेव्हा' असे म्हणतात, त्यांनी प्रवासाचे साधन स्पष्ट केलेले नाही." },
    { "प्रश्न_आयडी": 34, "प्रश्न": "चंद्रावर वजन कमी का होते?", "उत्तर": "चंद्राचे गुरुत्वाकर्षण पृथ्वीपेक्षा कमी असल्यामुळे तिथे वजन कमी वाटते." },
    { "प्रश्न_आयडी": 35, "प्रश्न": "ठसा हा शब्द कोणाशी संबंधित आहे?",
      "उत्तर": "ठसा हा शब्द नील आर्मस्ट्राँग यांनी चंद्रावर ठेवलेल्या पहिल्या पाऊलाशी संबंधित आहे." },
    { "प्रश्न_आयडी": 36, "प्रश्न": "कवीची चंद्रावरील स्थिती सांगा.", "उत्तर": "कवी चंद्रावर कापसासारखे हलके होऊन तरंगत हिंडत होते." },
    { "प्रश्न_आयडी": 37, "प्रश्न": "पौर्णिमा कधी असते?", "उत्तर": "ज्या रात्री चंद्र पूर्ण गोल दिसतो, त्या रात्रीला पौर्णिमा म्हणतात." },
    { "प्रश्न_आयडी": 38, "प्रश्न": "कवितेतील 'ससा-बिसा' या शब्दाचा उपयोग काय?", "उत्तर": "चंद्रावर ससा असतो, या जुन्या समजुतीला छेद देण्यासाठी हा शब्द वापरला आहे." },
    { "प्रश्न_आयडी": 39, "प्रश्न": "पृथ्वी कोठे उगवली होती?", "उत्तर": "पृथ्वी चंद्राच्या क्षितिजावर उगवली होती." },
    { "प्रश्न_आयडी": 40, "प्रश्न": "कवी चंद्रावर कशा प्रकारे हिंडले?", "उत्तर": "कवी चंद्रावर तरंगत तरंगत आरामात हिंडले." },
    { "प्रश्न_आयडी": 41, "प्रश्न": "चंद्रावर गेल्यावर कवीला पृथ्वीचे रूप कसे दिसले?", "उत्तर": "चंद्रावरून पृथ्वी निळ्या चांदण्यासारखी प्रकाशमान आणि सुंदर दिसली." },
