    { "प्रश्न_आयडी": 44, "प्रश्न": "'निळे चांदणे' हा शब्दप्रयोग कशासाठी केला आहे?", "उत्तर": "पृथ्वीच्या सौंदर्याचे वर्णन करण्यासाठी 'निळे चांदणे' हा शब्दप्रयोग केला आहे." },
    { "प्रश्न_आयडी": 45, "प्रश्न": "कवीला स्वतःचे शरीर कापूस का वाटले?", "उत्तर": "वजन अतिशय कमी झाल्यामुळे शरीर कापसासारखे हलके वाटले." },
    { "प्रश्न_आयडी": 46, "प्रश्न": "चंद्रावर पाणी असते का?", "उत्तर": "कवितेनुसार तिथे केवळ प्रकाश-अंधाराची रिमझिम होती, पाण्याचा उल्लेख नाही." },
    { "प्रश्न_आयडी": 47, "प्रश्न": "आर्मस्ट्राँगचा ठसा काय सिद्ध करतो?", "उत्तर": "तो मानवाचे चंद्रावरील पहिले पाऊल आणि विज्ञानाचे यश सिद्ध करतो." },
    { "प्रश्न_आयडी": 48, "प्रश्न": "तरंगणे ही क्रिया चंद्रावर का घडते?", "उत्तर": "गुरुत्वाकर्षण कमी असल्यामुळे तिथे कोणतीही वस्तू किंवा माणूस तरंगू शकतो." },
    { "प्रश्न_आयडी": 49, "प्रश्न": "कवितेत चंद्राचे वर्णन कसे केले आहे?", "उत्तर": "दिवस-रात्र नसलेला, प्रकाश-अंधाराची रिमझिम असलेला आणि आर्मस्ट्राँगचा ठसा असलेला असा चंद्राचा उल्लेख आहे." },
    { "प्रश्न_आयडी": 50, "प्रश्न": "कवीला इकडे तिकडे जाताना त्रास झाला का?", "उत्तर": "नाही, कवी तरंगत तरंगत आरामात हिंडले." },
    { "प्रश्न_आयडी": 51, "प्रश्न": "पौर्णिमा हसणे म्हणजे काय?", "उत्तर": "पौर्णिमेचे पूर्ण तेज चंद्रावर पडल्यामुळे ते वातावरण आनंदी वाटत होते." },
    { "प्रश्न_आयडी": 52, "प्रश्न": "पावसाची रिमझिम आणि प्रकाशाची रिमझिम यात काय फरक आहे?", "उत्तर": "पावसाची रिमझिम म्हणजे पाण्याचे थेंब पडणे, तर प्रकाशाची रिमझिम म्हणजे अंधुक प्रकाशाचे वातावरण असणे." },
    { "प्रश्न_आयडी": 53, "प्रश्न": "'इकडे तिकडे' या शब्दाचा वाक्यात उपयोग करा.", "उत्तर": "बागेत मुले इकडे तिकडे आनंदाने धावत होती." },
    { "प्रश्न_आयडी": 54, "प्रश्न": "'तरंगत तरंगत' या शब्दाचा वाक्यात उपयोग करा.", "उत्तर": "पाण्यात लाकडाचा ओंडका तरंगत तरंगत दूर गेला." },
    { "प्रश्न_आयडी": 55, "प्रश्न": "'ससा-बिसा' या शब्दाचा वाक्यात उपयोग करा.", "उत्तर": "जंगलात आम्हाला ससा-बिसा काहीच दिसले नाही." },
    { "प्रश्न_आयडी": 56, "प्रश्न": "पौर्णिमेच्या दिवशी आकाश कसे दिसते?", "उत्तर": "पौर्णिमेच्या दिवशी आकाश निरभ्र आणि प्रकाशमय दिसते." },
    { "प्रश्न_आयडी": 57, "प्रश्न": "ठसा म्हणजे काय?", "उत्तर": "कोणत्याही वस्तूला दाबल्यामुळे पृष्ठभागावर उमटलेली खूण म्हणजे ठसा." },
    { "प्रश्न_आयडी": 58, "प्रश्न": "चंद्रावर ससा का नसतो?", "उत्तर": "कारण चंद्रावर जगण्यासाठी आवश्यक हवा आणि पाणी नाही." },
    { "प्रश्न_आयडी": 59, "प्रश्न": "या कवितेतून कवीने कोणती भावना व्यक्त केली आहे?", "उत्तर": "या कवितेतून कवीने चंद्रावरील प्रवासाचे कुतूहल आणि आनंद व्यक्त केला आहे." },
    { "प्रश्न_आयडी": 60, "प्रश्न": "चंद्रावर गेल्यावर कवीने कोणाला नमस्कार केला असेल?", "उत्तर": "कवितेत तसा उल्लेख नाही, पण कवीने तिथे आर्मस्ट्राँगच्या ठशाचे कुतूहल व्यक्त केले आहे." }
  ]
}