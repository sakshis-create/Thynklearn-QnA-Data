  "लघु_उत्तरे": [
    { "प्रश्न_आयडी": 1, "प्रश्न": "सणानिमित्त कोण एकत्र आले होते?", "उत्तर": "सणानिमित्त सर्व चवी एकत्र आल्या होत्या." },
    { "प्रश्न_आयडी": 2, "प्रश्न": "साखर स्वतःबद्दल काय म्हणाली?", "उत्तर": "साखर म्हणाली की, तिला खूप मान आहे आणि प्रत्येक ठिकाणी तिचाच उपयोग होतो, म्हणून ती महान आहे." },
    { "प्रश्न_आयडी": 3, "प्रश्न": "चिंचेला पाहून साखरेने काय म्हटले?", "उत्तर": "साखरेने चिंचेला 'वाकडीतिकडी' म्हटले." },
    { "प्रश्न_आयडी": 4, "प्रश्न": "चिंचेचे नाव काढताच काय होते?", "उत्तर": "चिंचेचे नाव काढताच तोंडाला पाणी सुटते." },
    { "प्रश्न_आयडी": 5, "प्रश्न": "मिरचीने चिंचेला काय म्हटले?", "उत्तर": "मिरचीने चिंचेला 'वाकडीतिकडी' म्हटले आणि गप्प बसायला सांगितले." },
    { "प्रश्न_आयडी": 6, "प्रश्न": "मिरची कोठे मिरवते?", "उत्तर": "मिरची प्रत्येक घराघरांत मिरवते." },
    { "प्रश्न_आयडी": 7, "प्रश्न": "कारले रागात पुढे का आले?", "उत्तर": "मिरची खूप बडबड करत होती, म्हणून कारले रागात पुढे आले." },
    { "प्रश्न_आयडी": 8, "प्रश्न": "कारल्याचा उपयोग काय आहे?", "उत्तर": "कारल्याचा उपयोग औषध म्हणून केला जातो." },
    { "प्रश्न_आयडी": 9, "प्रश्न": "मिठाने साखरेबद्दल काय धोक्याचा इशारा दिला?", "उत्तर": "मिठाने सांगितले की जास्त गोड खाल्ल्याने रोग होतो." },
    { "प्रश्न_आयडी": 10, "प्रश्न": "चिंच जास्त खाल्ल्याने काय त्रास होतो?", "उत्तर": "चिंच जास्त खाल्ल्याने खोकला होतो." },
    { "प्रश्न_आयडी": 11, "प्रश्न": "मिरचीबद्दल मिठाचे काय मत होते?", "उत्तर": "मिरची सुद्धा जास्त खाऊ नये असा इशारा दिला जातो, असे मिठाने सांगितले." },
    { "प्रश्न_आयडी": 12, "प्रश्न": "जेवण बेचव कधी लागते?", "उत्तर": "जेवणात मीठ नसेल तर जेवण बेचव लागते." },
    { "प्रश्न_आयडी": 13, "प्रश्न": "लिंबाला पाहून काय झाले?", "उत्तर": "लिंबाला पाहून भांडणाऱ्या सर्व चवी गप्प झाल्या." },
    { "प्रश्न_आयडी": 14, "प्रश्न": "लिंबाने कोणाची बाजू घेतली?", "उत्तर": "लिंबाने कोणा एकाची बाजू न घेता सर्वांना समजावून सांगितले की प्रत्येक जण आपापल्या जागी महत्त्वाचा आहे." },
    { "प्रश्न_आयडी": 15, "प्रश्न": "आरोग्यासाठी कशाचा समतोल हवा?", "उत्तर": "आरोग्यासाठी सर्व चवींचा योग्य समतोल हवा." },
    { "प्रश्न_आयडी": 16, "प्रश्न": "चवींच्या भांडणाचा शेवट कसा झाला?", "उत्तर": "सर्व चवींनी आपली चूक मान्य केली आणि एकमेकांची क्षमा मागून भांडण संपवले." },
    { "प्रश्न_आयडी": 17, "प्रश्न": "जेवणाची लज्जत कशामुळे वाढते?", "उत्तर": "मिठामुळे जेवणाची लज्जत वाढते." },
    { "प्रश्न_आयडी": 18, "प्रश्न": "साखर महान आहे असे का वाटते?", "उत्तर": "साखर प्रत्येक गोड पदार्थात वापरली जाते म्हणून तिला मान आहे." },
    { "प्रश्न_आयडी": 19, "प्रश्न": "औषधात कशाचा वापर होतो?", "उत्तर": "औषधात कारल्याचा वापर होतो." },
    { "प्रश्न_आयडी": 20, "प्रश्न": "सर्व चवींनी आनंदाने काय केले?", "उत्तर": "सर्व चवींनी आनंदाने एकमेकांचे हात धरले." },
    { "प्रश्न_आयडी": 21, "प्रश्न": "'तोरा' या शब्दाचा अर्थ सांगा.", "उत्तर": "'तोरा' म्हणजे रुबाब किंवा ऐट." },
    { "प्रश्न_आयडी": 22, "प्रश्न": "मिरची कशी पुढे आली?", "उत्तर": "मिरची नाचत नाचत पुढे आली." },
    { "प्रश्न_आयडी": 23, "प्रश्न": "कारले कोठे बसले होते?", "उत्तर": "कारले कोपऱ्यात बसले होते." },
    { "प्रश्न_आयडी": 24, "प्रश्न": "'तावातावाने बोलणे' म्हणजे काय?", "उत्तर": "'तावातावाने बोलणे' म्हणजे खूप रागाने किंवा आवेशात बोलणे." },
    { "प्रश्न_आयडी": 25, "प्रश्न": "मिठाचे महत्त्व काय आहे?", "उत्तर": "मीठ अन्नाची चव वाढवते आणि शरीरासाठी आवश्यक आहे." },
    { "प्रश्न_आयडी": 26, "प्रश्न": "लिंबाची चव कशी असते?", "उत्तर": "लिंबाची चव आंबट असते." },
    { "प्रश्न_आयडी": 27, "प्रश्न": "भांडण चवींचे या पाठातून काय संदेश मिळतो?", "उत्तर": "सर्व घटक महत्त्वाचे असतात आणि एकत्र राहण्यातच मोठेपणा असतो." },
    { "प्रश्न_आयडी": 28, "प्रश्न": "मिरचीला 'वाकडीतिकडी' कोणी म्हटले?", "उत्तर": "चिंचेने मिरचीला वाकडीतिकडी म्हटले." },
    { "प्रश्न_आयडी": 29, "प्रश्न": "साखर कशात वापरली जाते?", "उत्तर": "साखर मिठाई, चहा आणि अनेक गोड पदार्थांत वापरली जाते." },
    { "प्रश्न_आयडी": 30, "प्रश्न": "मिठामुळे अन्नाचा काय वाढतो?", "उत्तर": "मिठामुळे अन्नाचा स्वाद आणि लज्जत वाढते." },
    { "प्रश्न_आयडी": 31, "प्रश्न": "कारले कशासाठी प्रसिद्ध आहे?", "उत्तर": "कारले त्याच्या कडू चवीसाठी आणि औषधी गुणांसाठी प्रसिद्ध आहे." },
    { "प्रश्न_आयडी": 32, "प्रश्न": "चिंच काय वाढवते?", "उत्तर": "चिंच जेवणाची आंबट चव वाढवते." },
    { "प्रश्न_आयडी": 33, "प्रश्न": "सर्व चवी कधी एकत्र आल्या होत्या?", "उत्तर": "सण झाल्यावर सर्वजण एकत्र आले होते." },
    { "प्रश्न_आयडी": 34, "प्रश्न": "साखरेचा अहंकार कोणी मोडला?", "उत्तर": "मिठाने साखरेचे दुष्परिणाम सांगून तिचा अहंकार मोडला." },
    { "प्रश्न_आयडी": 35, "प्रश्न": "मिरचीला जास्त खाल्ल्यावर काय होते?", "उत्तर": "मिरची जास्त खाल्ल्यावर पोटात आग होते आणि ती आरोग्यासाठी चांगली नसते." },
    { "प्रश्न_आयडी": 36, "प्रश्न": "प्रत्येक पदार्थाला कोणाचा मान असतो?", "उत्तर": "साखरेचा मान असतो असे तिने म्हटले." },
    { "प्रश्न_आयडी": 37, "प्रश्न": "'तोंड गोड करणे' म्हणजे काय?", "उत्तर": "काहीतरी गोड खाणे किंवा आनंदाची बातमी देणे." },
    { "प्रश्न_आयडी": 38, "प्रश्न": "कारले रागाने काय म्हणाले?", "उत्तर": "आमच्याशिवाय औषध अपूर्ण आहे असे कारले म्हणाले." },
    { "प्रश्न_आयडी": 39, "प्रश्न": "मिठाचे वैशिष्ट्य काय?", "उत्तर": "मीठ पांढरे असते आणि ते समुद्राच्या पाण्यापासून मिळते." },
    { "प्रश्न_आयडी": 40, "प्रश्न": "लिंबाचा उपयोग कशासाठी होतो?", "उत्तर": "लिंबू सरबत, लोणचे आणि जेवणात चव वाढवण्यासाठी वापरतात." },
    { "प्रश्न_आयडी": 41, "प्रश्न": "भांडण कोणत्या कारणावरून सुरू झाले?", "उत्तर": "प्रत्येक चव स्वतःला सर्वात श्रेष्ठ समजत होती, म्हणून भांडण सुरू झाले." },
    { "प्रश्न_आयडी": 42, "प्रश्न": "साखर महान का मानली जाते?", "उत्तर": "कारण सणासुदीला गोड पदार्थ महत्त्वाचे असतात." },
    { "प्रश्न_आयडी": 43, "प्रश्न": "मिठाला कोणी उत्तर दिले?", "उत्तर": "मिठाला लिंबाने सर्वांसोबत उत्तर दिले." },
    { "प्रश्न_आयडी": 44, "प्रश्न": "चिंचेचे रूप कसे आहे?", "उत्तर": "चिंच वाकडीतिकडी असते." },
    { "प्रश्न_आयडी": 45, "प्रश्न": "मिरचीचा रंग कोणता असतो?", "उत्तर": "मिरचीचा रंग हिरवा किंवा लाल असतो." },
    { "प्रश्न_आयडी": 46, "प्रश्न": "कारल्याची चव कशी असते?", "उत्तर": "कारल्याची चव कडू असते." },
    { "प्रश्न_आयडी": 47, "प्रश्न": "जेवणात मीठ नसेल तर त्याला काय म्हणतात?", "उत्तर": "जेवणात मीठ नसेल तर त्याला 'अळणी' म्हणतात." },
    { "प्रश्न_आयडी": 48, "प्रश्न": "लिंबाने कोणाला नमस्कार केला?", "उत्तर": "लिंबाने सर्व चवींना आदराने नमस्कार केला." },
    { "प्रश्न_आयडी": 49, "प्रश्न": "भांडण थांबवण्यासाठी कोणाची गरज होती?", "उत्तर": "भांडण थांबवण्यासाठी एका समजूतदार मध्यस्थाची (लिंबाची) गरज होती." },
    { "प्रश्न_आयडी": 50, "प्रश्न": "चवी किती प्रकारच्या असतात?", "उत्तर": "मुख्यतः सहा चवी असतात (गोड, आंबट, तिखट, कडू, खारट, तुरट)." },
    { "प्रश्न_आयडी": 51, "प्रश्न": "मिरचीचे कार्य काय आहे?", "उत्तर": "पदार्थाला तिखटपणा आणि झणझणीत चव देणे." },
    { "प्रश्न_आयडी": 52, "प्रश्न": "चिंच कोणासोबत भांडत होती?", "उत्तर": "चिंच प्रामुख्याने साखरेसोबत भांडत होती." },
    { "प्रश्न_आयडी": 53, "प्रश्न": "साखर कोठे मिरवते?", "उत्तर": "साखर प्रत्येक शुभवार्तेच्या वेळी मिरवते." },
    { "प्रश्न_आयडी": 54, "प्रश्न": "मिठामुळे आरोग्यावर काय परिणाम होतो?", "उत्तर": "योग्य प्रमाणात मीठ शरीरासाठी आवश्यक आहे, पण जास्त मिठामुळे रक्तदाब वाढू शकतो." },
    { "प्रश्न_आयडी": 55, "प्रश्न": "कारल्याचे महत्त्व कधी कळते?", "उत्तर": "जेव्हा माणूस आजारी पडतो तेव्हा औषधात कारल्याचे महत्त्व कळते." },
    { "प्रश्न_आयडी": 56, "प्रश्न": "लिंबू कसा आहे?", "उत्तर": "लिंबू गोल आणि पिवळ्या रंगाचा असतो." },
    { "प्रश्न_आयडी": 57, "प्रश्न": "क्षमा मागणे म्हणजे काय?", "उत्तर": "आपली चूक मान्य करून माफी मागणे." },
    { "प्रश्न_आयडी": 58, "प्रश्न": "एकतेचा फायदा काय?", "उत्तर": "एकत्र राहिल्याने कोणत्याही कामात यश मिळते आणि जेवण चविष्ट लागते." },
    { "प्रश्न_आयडी": 39, "प्रश्न": "साखर कोठे विरघळते?", "उत्तर": "साखर पाण्यात किंवा दुधात सहज विरघळते." },
    { "प्रश्न_आयडी": 60, "प्रश्न": "चिंचेचे लोणचे कसे लागते?", "उत्तर": "चिंचेचे लोणचे आंबट-गोड आणि चविष्ट लागते." },
    { "प्रश्न_आयडी": 61, "प्रश्न": "मिरचीचा तिखटपणा कोठे जाणवतो?", "उत्तर": "मिरचीचा तिखटपणा जिभेवर जाणवतो." },
    { "प्रश्न_आयडी": 62, "प्रश्न": "मिठाचे दुसरे नाव काय?", "उत्तर": "मिठाला 'लवण' असेही म्हणतात." },
    { "प्रश्न_आयडी": 63, "प्रश्न": "कारल्याची भाजी सर्वांना आवडते का?", "उत्तर": "कडू चवीमुळे कारल्याची भाजी सर्वांना आवडत नाही, पण ती आरोग्यदायी असते." },
    { "प्रश्न_आयडी": 64, "प्रश्न": "सण झाल्यावर काय झाले?", "उत्तर": "सण झाल्यावर सर्वजण एकत्र येऊन गप्पा मारू लागले आणि मग भांडण सुरू झाले." },
    { "प्रश्न_आयडी": 65, "प्रश्न": "लिंबाने कोणाला गप्प केले?", "उत्तर": "लिंबाने तावातावाने बोलणाऱ्या कारल्याला आणि मिरवते म्हणणाऱ्या मिरचीला गप्प केले." },
    { "प्रश्न_आयडी": 66, "प्रश्न": "चवींचा समतोल का महत्त्वाचा आहे?", "उत्तर": "कारण एकाच चवीचा अतिवापर शरीराला घातक ठरू शकतो." },
    { "प्रश्न_आयडी": 67, "प्रश्न": "साखरेचा समानार्थी शब्द लिहा.", "उत्तर": "साखरेला शर्करा असेही म्हणतात." },
    { "प्रश्न_आयडी": 68, "प्रश्न": "पाठातील मुख्य पात्रे कोण आहेत?", "उत्तर": "साखर, चिंच, मिरची, कारले, मीठ आणि लिंबू ही मुख्य पात्रे आहेत." },
    { "प्रश्न_आयडी": 69, "प्रश्न": "चिंचेला काय बसतो?", "उत्तर": "चिंच खाल्ल्यावर घसा धरतो आणि आईचा मार बसतो असे मिठाने गमतीने म्हटले." },
    { "प्रश्न_आयडी": 70, "प्रश्न": "शेवटी सर्वजण कसे राहू लागले?", "उत्तर": "शेवटी सर्व चवी मिळून मिसळून आनंदाने राहू लागल्या." }
  ]
}