  "लघु_उत्तरे": [
    { "प्रश्न_आयडी": 1, "प्रश्न": "कमळबन नावाचे जंगल कोठे होते?", "उत्तर": "एका सरोवराकाठी घनदाट जंगल होते, त्याला कमळबन म्हणत." },
    { "प्रश्न_आयडी": 2, "प्रश्न": "कमळबनाचे वैशिष्ट्य काय होते?", "उत्तर": "तेथील सरोवर नेहमी कमळांनी फुललेले असायचे." },
    { "प्रश्न_आयडी": 3, "प्रश्न": "हरणाचे कुटुंब कोठे राहत होते?", "उत्तर": "हरणाचे कुटुंब कमळबन नावाच्या जंगलात राहत होते." },
    { "प्रश्न_आयडी": 4, "प्रश्न": "पाडसे कशी होती?", "उत्तर": "पाडसे खूप खेळकर होती आणि ती एकमेकांना सोडून कुठेही जात नसत." },
    { "प्रश्न_आयडी": 5, "प्रश्न": "हरणांच्या कुटुंबात कोणाची भीती होती?", "उत्तर": "त्यांच्या मनात जंगली पशूंची भीती होती." },
    { "प्रश्न_आयडी": 6, "प्रश्न": "झाडाच्या मागून काय चमकताना दिसले?", "उत्तर": "झाडाच्या मागून वाघाचे दोन डोळे चमकताना दिसले." },
    { "प्रश्न_आयडी": 7, "प्रश्न": "वाघाला पाहून कुटुंबाला काय वाटले?", "उत्तर": "आता चौघांपैकी एकाला मरावे लागणार, असे कुटुंबाला वाटले." },
    { "प्रश्न_आयडी": 8, "प्रश्न": "हरिणीने पती व मुलांसाठी काय म्हटले?", "उत्तर": "हरिणीने म्हटले, 'तुम्ही तिघे इथून जा. मी वाघासाठी मागे थांबते (स्वतःचे बलिदान देते).'" },
    { "प्रश्न_आयडी": 9, "प्रश्न": "हरिणाने काय बलिदान देण्याचे ठरवले?", "उत्तर": "हरिण म्हणाले, 'नको, नको, तुम्ही तिघे जा. मी मागे राहतो.'" },
    { "प्रश्न_आयडी": 10, "प्रश्न": "पाडसाने आपल्या पालकांसाठी काय म्हटले?", "उत्तर": "पाडस म्हणाले, 'नको, नको तुम्ही सगळे निघून जा. मी मागे राहतो.'" },
    { "प्रश्न_आयडी": 11, "प्रश्न": "वाघावर हरणांच्या या वागण्याचा काय परिणाम झाला?", "उत्तर": "हरणांचे एकमेकांवरील प्रेम पाहून वाघाचे हृदय विरघळले आणि त्याचे मन परिवर्तन झाले." },
    { "प्रश्न_आयडी": 12, "प्रश्न": "वाघाने शेवटी काय केले?", "उत्तर": "वाघाने हरणाची शिकार केली नाही आणि तो गुपचूप दऱ्याखोऱ्यात निघून गेला." },
    { "प्रश्न_आयडी": 13, "प्रश्न": "कुंभकोणम् येथील पाठशाळेत कोणता तास सुरू होता?", "उत्तर": "कुंभकोणम् येथील पाठशाळेत गणिताचा तास सुरू होता." },
    { "प्रश्न_आयडी": 14, "प्रश्न": "शिक्षक कोणता सिद्धांत शिकवत होते?", "उत्तर": "एखाद्या संख्येला त्याच संख्येने भागले असता भागाकार नेहमी 'एक' येतो, हा सिद्धांत शिक्षक शिकवत होते." },
    { "प्रश्न_आयडी": 15, "प्रश्न": "मुलाने शिक्षकांना काय विचारले?", "उत्तर": "मुलाने विचारले, 'गुरुजी, शून्याला शून्याने भागले तर उत्तर एक येईल का?'" },
    { "प्रश्न_आयडी": 16, "प्रश्न": "शिक्षकांनी चष्म्यातून कोणाकडे पाहिले?", "उत्तर": "शिक्षकांनी चष्म्यातून त्या धाडसी आणि हुशार मुलाकडे पाहिले." },
    { "प्रश्न_आयडी": 17, "प्रश्न": "मुलाने शिक्षकांचा सिद्धांत चुकीचा का ठरवला?", "उत्तर": "कारण 'शून्य' या संख्येच्या बाबतीत तो सिद्धांत लागू होत नव्हता." },
    { "प्रश्न_आयडी": 18, "प्रश्न": "जंगलात पशुपक्ष्यांची कोणकोणती घरे होती?", "उत्तर": "जंगलात गुहा, घरटी, बिळे, वारूळ आणि जाळी अशी पशुपक्ष्यांची घरे होती." },
    { "प्रश्न_आयडी": 19, "प्रश्न": "हरणाचे कुटुंब सरोवराकाठी कशासाठी निघाले होते?", "उत्तर": "हरणाचे कुटुंब सरोवराकाठी पाणी पिण्यासाठी निघाले होते." },
    { "प्रश्न_आयडी": 20, "प्रश्न": "पाडसांना एकमेकांबद्दल काय होता?", "उत्तर": "पाडसांना एकमेकांबद्दल खूप जिव्हाळा होता." },
    { "प्रश्न_आयडी": 21, "प्रश्न": "वाघाचे डोळे कोठून चमकत होते?", "उत्तर": "वाघाचे डोळे एका मोठ्या झाडाच्या मागून चमकत होते." },
    { "प्रश्न_आयडी": 22, "प्रश्न": "हरिणीचा त्याग काय दर्शवतो?", "उत्तर": "हरिणीचा त्याग तिच्या कुटुंबाप्रती असलेले तिचे अपार प्रेम दर्शवतो." },
    { "प्रश्न_आयडी": 23, "प्रश्न": "वाघ रागात होता का?", "उत्तर": "वाघ सुरुवातीला शिकारीच्या तयारीत होता, पण नंतर तो शांत झाला." },
    { "प्रश्न_आयडी": 24, "प्रश्न": "हरणाच्या कुटुंबाला 'सुखी कुटुंब' का म्हटले आहे?", "उत्तर": "कारण ते एकमेकांवर प्रेम करत होते आणि संकटात एकमेकांसाठी जीव द्यायला तयार होते." },
    { "प्रश्न_आयडी": 25, "प्रश्न": "गणिताच्या शिक्षकांचा चेहरा कसा झाला?", "उत्तर": "गणिताच्या शिक्षकांचा चेहरा विलक्षण गंभीर झाला." },
    { "प्रश्न_आयडी": 26, "प्रश्न": "मुलाने आपला प्रश्न कशा प्रकारे मांडला?", "उत्तर": "मुलाने आपला प्रश्न ताडकन उभे राहून आणि खड्या आवाजात मांडला." },
    { "प्रश्न_आयडी": 27, "प्रश्न": "सरोवराचे पाणी कसे चमकत होते?", "उत्तर": "सरोवराचे पाणी निळेशार चमकत होते." },
    { "प्रश्न_आयडी": 28, "प्रश्न": "त्याग करण्याची भावना केव्हा येते?", "उत्तर": "जेव्हा मनात दुसऱ्याबद्दल अपार प्रेम असते, तेव्हा त्याग करण्याची भावना येते." },
    { "प्रश्न_आयडी": 29, "प्रश्न": "वाघाचे हृदय का विरघळले?", "उत्तर": "हरिण, हरिणी आणि पाडसांचे एकमेकांसाठी मरायला तयार होण्याचे प्रेम पाहून वाघाचे हृदय विरघळले." },
    { "प्रश्न_आयडी": 30, "प्रश्न": "जंगलाचे वर्णन दोन वाक्यांत करा.", "उत्तर": "जंगल घनदाट होते आणि ते पशुपक्षी, झाडेझुडपे, डोंगरदऱ्या यांनी वेढलेले होते." },
    { "प्रश्न_आयडी": 31, "प्रश्न": "'जिव्हाळा' या शब्दाचा वाक्यात उपयोग करा.", "उत्तर": "आईला आपल्या मुलाबद्दल खूप जिव्हाळा असतो." },
    { "प्रश्न_आयडी": 32, "प्रश्न": "शिक्षक कोणत्या गावात शिकवत होते?", "उत्तर": "शिक्षक कुंभकोणम् येथील एका पाठशाळेत शिकवत होते." },
    { "प्रश्न_आयडी": 33, "प्रश्न": "मुलांनी गणिती सिद्धांत कोठे लिहून घेतला?", "उत्तर": "मुलांनी गणिती सिद्धांत आपल्या वह्यांमध्ये टिपून घेतला." },
    { "प्रश्न_आयडी": 34, "प्रश्न": "शिक्षकांचा आवाज कसा होता?", "उत्तर": "शिक्षकांचा आवाज काहीसा करडा होता." },
    { "प्रश्न_आयडी": 35, "प्रश्न": "पाडसे आनंदाने काय करत होती?", "उत्तर": "पाडसे आनंदाने गाणी गात, खेळत आणि बागडत होती." },
    { "प्रश्न_आयडी": 36, "प्रश्न": "वाघाला कोठे जायचे होते?", "उत्तर": "वाघ शेवटी दऱ्याखोऱ्यात निघून गेला." },
    { "प्रश्न_आयडी": 37, "प्रश्न": "हरणाच्या कुटुंबात किती पाडसे होती?", "उत्तर": "हरणाच्या कुटुंबात दोन पाडसे होती." },
    { "प्रश्न_आयडी": 38, "प्रश्न": "जंगलातील ओढे-नाल्यांचे वर्णन करा.", "उत्तर": "जंगलात ओढे-नाले आणि डोंगरदऱ्यांनी वेढलेले निसर्गरम्य वातावरण होते." },
    { "प्रश्न_आयडी": 39, "प्रश्न": "शिक्षक कोणत्या विषयाचा तास घेत होते?", "उत्तर": "शिक्षक गणिताचा तास घेत होते." },
    { "प्रश्न_आयडी": 40, "प्रश्न": "मुलाचे प्रश्न विचारणे काय दर्शवते?", "उत्तर": "मुलाचे प्रश्न विचारणे त्याची जिज्ञासा आणि कुशाग्र बुद्धिमत्ता दर्शवते." },
    { "प्रश्न_आयडी": 41, "प्रश्न": "कमळबनात सरोवराचे पाणी कसे होते?", "उत्तर": "सरोवराचे पाणी स्वच्छ आणि निळेशार होते." },
    { "प्रश्न_आयडी": 42, "प्रश्न": "हरणाचे कुटुंब पाणी पिण्यासाठी सरोवराकडे का गेले?", "उत्तर": "कारण त्यांना तहान लागली होती." },
    { "प्रश्न_आयडी": 43, "प्रश्न": "वाघाने झडप का घातली नाही?", "उत्तर": "हरणांमधील अपार प्रेम पाहून वाघाची शिकार करण्याची इच्छा संपली." },
    { "प्रश्न_आयडी": 44, "प्रश्न": "धड्यातील कथेचा शेवट कसा झाला?", "उत्तर": "कथेचा शेवट सकारात्मक झाला, वाघाने कोणालाही इजा केली नाही." },
    { "प्रश्न_आयडी": 45, "प्रश्न": "मायाळू म्हणजे काय?", "उत्तर": "दुसऱ्याबद्दल प्रेम आणि दया बाळगणारी व्यक्ती म्हणजे मायाळू." },
    { "प्रश्न_आयडी": 46, "प्रश्न": "वाघाचे डोळे रात्रीच्या वेळी कसे दिसतात?", "उत्तर": "वाघाचे डोळे अंधारात चमकताना दिसतात." },
    { "प्रश्न_आयडी": 47, "प्रश्न": "शिक्षकांनी मुलाला आपले म्हणणे कसे स्पष्ट करायला सांगितले?", "उत्तर": "शिक्षकांनी मुलाला खड्या आवाजात आपले म्हणणे स्पष्ट करायला सांगितले." },
    { "प्रश्न_आयडी": 48, "प्रश्न": "हरिण आणि हरिणीमध्ये कोणाचे प्रेम जास्त होते?", "उत्तर": "दोघांचेही एकमेकांवर आणि मुलांवर सारखेच अपार प्रेम होते." },
    { "प्रश्न_आयडी": 49, "प्रश्न": "शून्याचा भागाकार काय होतो?", "उत्तर": "शून्याला शून्याने भागले तर त्याचे उत्तर निश्चित नसते (अव्याख्यित असते)." },
    { "प्रश्न_आयडी": 50, "प्रश्न": "जंगलात कोणकोणते प्राणी राहतात?", "उत्तर": "जंगलात वाघ, हरिण, पशुपक्षी आणि इतर जंगली प्राणी राहतात." },
    { "प्रश्न_आयडी": 51, "प्रश्न": "सरोवरातील कमळांचा रंग कोणता असतो?", "उत्तर": "सरोवरातील कमळे गुलाबी किंवा पांढऱ्या रंगाची असू शकतात." },
    { "प्रश्न_आयडी": 52, "प्रश्न": "पाडसे कशी बागडत होती?", "उत्तर": "पाडसे आनंदाने उड्या मारत आणि खेळत बागडत होती." },
    { "प्रश्न_आयडी": 53, "प्रश्न": "शिक्षकांनी चष्म्याचा वापर का केला?", "उत्तर": "शिक्षकांना नीट दिसावे म्हणून त्यांनी जाड भिंगाचा चष्मा वापरला होता." },
    { "प्रश्न_आयडी": 54, "प्रश्न": "सिद्धांत टिपून घेणे म्हणजे काय?", "उत्तर": "शिक्षकांनी सांगितलेला महत्त्वाचा नियम वहीत लिहून ठेवणे." },
    { "प्रश्न_आयडी": 55, "प्रश्न": "वाघाचे मनपरिवर्तन ही एक काय आहे?", "उत्तर": "ही एक महत्त्वाची घटना आहे जी प्रेमाची ताकद दर्शवते." },
    { "प्रश्न_आयडी": 56, "प्रश्न": "हरणाचे कुटुंब कोणत्या सरोवराकाठी होते?", "उत्तर": "हरणाचे कुटुंब कमळबन नावाच्या सरोवराकाठी होते." },
    { "प्रश्न_आयडी": 57, "प्रश्न": "पाडसांचा जिव्हाळा कशातून दिसला?", "उत्तर": "संकट आल्यावर आई-बाबांना वाचवण्यासाठी स्वतः पुढे येण्यावरून त्यांचा जिव्हाळा दिसला." },
    { "प्रश्न_आयडी": 58, "प्रश्न": "पाठशाळेतील गणिताचा सिद्धांत कोणाला समजला?", "उत्तर": "पाठशाळेतील सिद्धांत वर्गातील सर्व मुलांना समजला, पण एका मुलाने त्यावर प्रश्न विचारला." },
    { "प्रश्न_आयडी": 59, "प्रश्न": "या कथेचे नाव 'अपार प्रेम' का योग्य आहे?", "उत्तर": "कारण संपूर्ण कथा हरणांच्या कुटुंबातील निस्वार्थ प्रेमाभोवती फिरते." },
    { "प्रश्न_आयडी": 60, "प्रश्न": "या पाठातून आपल्याला कोणता संदेश मिळतो?", "उत्तर": "एकमेकांवर प्रेम केल्यास आणि कठीण प्रसंगी सोबत राहिल्यास मोठ्या संकटावरही मात करता येते." }
  ]
}