  "लघु_उत्तरे": [
    { "प्रश्न_आयडी": 1, "प्रश्न": "सुधाताई प्रतीकला काय म्हणाल्या?", "उत्तर": "सुधाताई म्हणाल्या, 'प्रतीक, आता उठ बरं, सात वाजलेसुद्धा!'" },
    { "प्रश्न_आयडी": 2, "प्रश्न": "प्रतीक उठल्यावर कुठे आला?", "उत्तर": "प्रतीक डोळे चोळत गॅलरीत आला." },
    { "प्रश्न_आयडी": 3, "प्रश्न": "बाहेरचे वातावरण पाहून प्रतीकला काय वाटले?", "उत्तर": "बाहेरचे प्रसन्न वातावरण पाहून त्याला उत्साहाचे पंख फुटले." },
    { "प्रश्न_आयडी": 4, "प्रश्न": "प्रतीकने नदीला कोणता प्रश्न विचारला?", "उत्तर": "प्रतीकने विचारले, 'नदीताई, तू दिवस-रात्र धावून थकते कशी नाहीस?'" },
    { "प्रश्न_आयडी": 5, "प्रश्न": "नदीने प्रवासाचे महत्त्व कसे सांगितले?", "उत्तर": "नदी म्हणाली, 'धावणे हाच माझा प्राण आणि श्वास आहे.'" },
    { "प्रश्न_आयडी": 6, "प्रश्न": "नदीला कशात आनंद मिळतो?", "उत्तर": "नदीला सतत पुढे जाण्यात आनंद मिळतो." },
    { "प्रश्न_आयडी": 7, "प्रश्न": "नदीचा छंद कोणता आहे?", "उत्तर": "निर्मलता मिळवणे हा नदीचा छंद आहे." },
    { "प्रश्न_आयडी": 8, "प्रश्न": "वाऱ्याला कशाचा थकवा येत नाही?", "उत्तर": "वाऱ्याला सतत वाहण्याचा थकवा येत नाही." },
    { "प्रश्न_आयडी": 9, "प्रश्न": "वारा डोंगराचे काय करतो?", "उत्तर": "वारा डोंगरदऱ्यांवरील दगडांची धूळ झटकून त्यांची तकाकी वाढवतो." },
    { "प्रश्न_आयडी": 10, "प्रश्न": "सूर्याचा उत्साह कोणाकडून मिळतो?",
      "उत्तर": "सूर्याचा उत्साह त्याच्या स्वतःच्या प्रकाशातून आणि सातत्यपूर्ण कार्यातून मिळतो." },
    { "प्रश्न_आयडी": 11, "प्रश्न": "कवी कुठे राहायचा?", "उत्तर": "कवी एका लहान खेडेगावात राहायचा." },
    { "प्रश्न_आयडी": 12, "प्रश्न": "कवीने मिठाईवर कविता का केली?", "उत्तर": "मिठाईचे दुकान पाहून त्याला स्फूर्ती मिळाली म्हणून त्याने कविता केली." },
    { "प्रश्न_आयडी": 13, "प्रश्न": "मिठाई विक्रेत्याने कवितेचा काय उपयोग केला?", "उत्तर": "मिठाई विक्रेत्याने कवितेच्या कागदावर लाडू देऊन तो कागद खराब केला." },
    { "प्रश्न_आयडी": 14, "प्रश्न": "कवीला दुःख का झाले?", "उत्तर": "कवितेचा कागद कचरा समजून वापरला गेल्याने कवीला दुःख झाले." },
    { "प्रश्न_आयडी": 15, "प्रश्न": "भांडी विक्रेत्याच्या दुकानाजवळ काय घडले?", "उत्तर": "भांडी ठोकण्याच्या आवाजामुळे कवीची कविता कोणाला ऐकू आली नाही." },
    { "प्रश्न_आयडी": 16, "प्रश्न": "कवी माठ आणायला कुठे गेला?", "उत्तर": "कवी माठ आणायला कुंभाराच्या घरी गेला." },
    { "प्रश्न_आयडी": 17, "प्रश्न": "कुंभार काय करत होता?", "उत्तर": "कुंभार चाकावर मातीला आकार देऊन सुरेख माठ घडवत होता." },
    { "प्रश्न_आयडी": 18, "प्रश्न": "कवीला खरी कविता कुठे गवसली?", "उत्तर": "कुंभाराच्या कष्टाच्या घामात कवीला खरी कविता गवसली." },
    { "प्रश्न_आयडी": 19, "प्रश्न": "नदीचे पाणी कसे असते?", "उत्तर": "नदीचे पाणी स्वच्छ आणि निर्मळ असते." },
    { "प्रश्न_आयडी": 20, "प्रश्न": "निसर्ग आपल्याला काय शिकवतो?", "उत्तर": "निसर्ग आपल्याला सतत कार्यरत राहण्याचा आणि पुढे जाण्याचा संदेश देतो." },
    { "प्रश्न_आयडी": 21, "प्रश्न": "प्रतीकने आईला काय सांगितले?", "उत्तर": "प्रतीक म्हणाला, 'आई, मी जरा फिरून येतो.'" },
    { "प्रश्न_आयडी": 22, "प्रश्न": "वातावरणातील प्रसन्नतेचा परिणाम काय झाला?", "उत्तर": "प्रसन्नतेमुळे प्रतीकचा आळस गेला आणि तो उत्साही झाला." },
    { "प्रश्न_आयडी": 23, "प्रश्न": "नदी कोठून वाहते?", "उत्तर": "नदी डोंगरदऱ्यातून आणि मैदानांतून वाहते." },
    { "प्रश्न_आयडी": 24, "प्रश्न": "कवितेचे महत्त्व काय असते?", "उत्तर": "कविता भावना व्यक्त करण्याचे एक प्रभावी माध्यम असते." },
    { "प्रश्न_आयडी": 25, "प्रश्न": "कवीच्या मुलाने काय आणले?", "उत्तर": "कवीच्या मुलाने मिठाईच्या दुकानातून लाडू आणला." },
    { "प्रश्न_आयडी": 26, "प्रश्न": "लाडू कोणत्या कागदात होता?", "उत्तर": "लाडू कवीने लिहिलेल्या कवितेच्या कागदात होता." },
    { "प्रश्न_आयडी": 27, "प्रश्न": "भांडी विक्रेत्याचे लक्ष कुठे होते?", "उत्तर": "भांडी विक्रेत्याचे लक्ष भांडी ठोकण्यावर आणि कामावर होते." },
    { "प्रश्न_आयडी": 28, "प्रश्न": "कुंभाराच्या चाकाचे वैशिष्ट्य काय?", "उत्तर": "कुंभाराचे चाक फिरून मातीच्या गोळ्याला सुंदर आकार देते." },
    { "प्रश्न_आयडी": 29, "प्रश्न": "घामाचे महत्त्व काय?", "उत्तर": "प्रामाणिक कष्टाचा घाम यशाचे आणि समाधानाचे प्रतीक असतो." },
    { "प्रश्न_आयडी": 30, "प्रश्न": "सूर्याची किरणे काय करतात?", "उत्तर": "सूर्याची किरणे अंधार दूर करून जगाला प्रकाश देतात." },
    { "प्रश्न_आयडी": 31, "प्रश्न": "'इकडे आड तिकडे विहीर' या म्हणीचा अर्थ सांगा.", "उत्तर": "दोन्ही बाजूंनी अडचणीत सापडणे." },
    { "प्रश्न_आयडी": 32, "प्रश्न": "'बाप तसा बेटा' म्हणजे काय?", "उत्तर": "मुलामध्ये वडिलांसारखेच गुण असणे." },
    { "प्रश्न_आयडी": 33, "प्रश्न": "गाव करी ते राव न करी' या म्हणीचा अर्थ सांगा.", "उत्तर": "जे काम एकजूट होऊन लोक करू शकतात, ते श्रीमंत व्यक्ती एकटी करू शकत नाही." },
    { "प्रश्न_आयडी": 34, "प्रश्न": "नदीला काय आवडते?", "उत्तर": "नदीला खळाळत वाहणे आणि पुढे जाणे आवडते." },
    { "प्रश्न_आयडी": 35, "प्रश्न": "वाऱ्याचा वेग कसा असतो?", "उत्तर": "वारा कधी मंद तर कधी वादळासारखा वेगाने वाहतो." },
    { "प्रश्न_आयडी": 36, "प्रश्न": "प्रतीकच्या मनातील स्वप्न कोणते?", "उत्तर": "निसर्गाशी संवाद साधण्याचे आणि आळस सोडून पुढे जाण्याचे प्रतीकचे स्वप्न होते." },
    { "प्रश्न_आयडी": 37, "प्रश्न": "नदीचा श्वास काय आहे?", "उत्तर": "प्रवाह टिकवून ठेवणे हाच नदीचा श्वास आहे." },
    { "प्रश्न_आयडी": 38, "प्रश्न": "प्रतीक बाहेर जाताना कोणाला भेटला?", "उत्तर": "प्रतीक पक्षी, नदी आणि वाऱ्याला भेटला." },
    { "प्रश्न_आयडी": 39, "प्रश्न": "कवीला कोणाचा राग आला नाही?", "उत्तर": "कवीला प्रत्यक्षात कोणाचा राग आला नाही, पण त्याला दुःख नक्कीच झाले." },
    { "प्रश्न_आयडी": 40, "प्रश्न": "सूर्याचे कार्य काय आहे?", "उत्तर": "सूर्याचे कार्य विश्वाला ऊर्जा आणि प्रकाश देणे हे आहे." },
    { "प्रश्न_आयडी": 41, "प्रश्न": "मिठाई विक्रेत्याने कवीची कविता का वाचली नाही?", "उत्तर": "कारण त्याला कवितेपेक्षा आपला धंदा आणि विक्री जास्त महत्त्वाची वाटली." },
    { "प्रश्न_आयडी": 42, "प्रश्न": "कष्टाची कविता म्हणजे काय?", "उत्तर": "प्रत्यक्ष कामातून निर्माण झालेली निर्मिती म्हणजे कष्टाची कविता." },
    { "प्रश्न_आयडी": 43, "प्रश्न": "नदी थकल्यासारखी का वाटत नाही?", "उत्तर": "कारण ती सतत नवीन उत्साह शोधत असते आणि थांबत नाही." },
    { "प्रश्न_आयडी": 44, "प्रश्न": "प्रतीकच्या आईला आश्चर्य का वाटले?", "उत्तर": "प्रतीक लवकर उठून बाहेर फिरून येतो म्हटल्यावर आईला आश्चर्य वाटले." },
    { "प्रश्न_आयडी": 45, "प्रश्न": "गॅलरीतून काय दिसत होते?", "उत्तर": "गॅलरीतून उगवणारा सूर्य आणि सुंदर निसर्ग दिसत होता." },
    { "प्रश्न_आयडी": 46, "प्रश्न": "पाण्याशिवाय नदी काय आहे?", "उत्तर": "पाण्याशिवाय नदीचे अस्तित्व संपते, पण पाण्याचा प्रवाह हीच तिची ओळख आहे." },
    { "प्रश्न_आयडी": 47, "प्रश्न": "वाऱ्याचा संदेश काय आहे?", "उत्तर": "गती हेच जीवन आहे, हा वाऱ्याचा संदेश आहे." },
    { "प्रश्न_आयडी": 48, "प्रश्न": "कवीच्या मनात कविता कशी सुचते?", "उत्तर": "सभोवतालच्या वस्तू आणि निसर्ग पाहून कवीला कविता सुचते." },
    { "प्रश्न_आयडी": 49, "प्रश्न": "लाडू खाताना कवीला काय दिसले?", "उत्तर": "लाडू खाताना कवीला स्वतःची कविता त्या कागदावर दिसली." },
    { "प्रश्न_आयडी": 50, "प्रश्न": "कुंभाराचा संयम कसा असतो?", "उत्तर": "कुंभार अतिशय संयमाने आणि हळुवार हाताने मातीला आकार देतो." },
    { "प्रश्न_आयडी": 51, "प्रश्न": "कवीला माठ का हवा होता?", "उत्तर": "उन्हाळ्यात थंड पाणी मिळण्यासाठी कवीला माठ हवा होता." },
    { "प्रश्न_आयडी": 52, "प्रश्न": "कविता कागदावर असण्यापेक्षा कुठे हवी?", "उत्तर": "कविता कागदावर असण्यापेक्षा लोकांच्या आचरणात आणि कष्टांत हवी." },
    { "प्रश्न_आयडी": 53, "प्रश्न": "नदी कोणाला पाणी देते?", "उत्तर": "नदी वाटेत येणाऱ्या सर्व प्राणी, पक्षी आणि झाडांना पाणी देते." },
    { "प्रश्न_आयडी": 54, "प्रश्न": "सूर्याला थकवा का येत नाही?", "उत्तर": "नियमबद्धता आणि परोपकार यांमुळे सूर्याला थकवा येत नाही." },
    { "प्रश्न_आयडी": 55, "प्रश्न": "प्रतीक कोणत्या वर्गात असेल?", "उत्तर": "धडा इयत्ता सहावीचा असल्याने प्रतीक अंदाजे १०-११ वर्षांचा असेल." },
    { "प्रश्न_आयडी": 56, "प्रश्न": "नदीला प्रतीकने काय म्हटले?", "उत्तर": "प्रतीकने नदीला 'नदीताई' असे प्रेमाने म्हटले." },
    { "प्रश्न_आयडी": 57, "प्रश्न": "वाऱ्याचा स्पर्श कसा असतो?", "उत्तर": "वाऱ्याचा स्पर्श शीतल आणि सुखावह असतो." },
    { "प्रश्न_आयडी": 58, "प्रश्न": "कवीचे जीवन कसे होते?", "उत्तर": "कवीचे जीवन साधे आणि निसर्गाच्या सानिध्यात होते." },
    { "प्रश्न_आयडी": 59, "प्रश्न": "कामाचा गौरव कोठे होतो?", "उत्तर": "कामाचा गौरव त्याच्या फळात आणि उपयुक्ततेत होतो." },
    { "प्रश्न_आयडी": 60, "प्रश्न": "म्हणींचे महत्त्व काय?", "उत्तर": "कमी शब्दात मोठा अर्थ आणि अनुभव सांगण्यासाठी म्हणींचे महत्त्व आहे." },
    { "प्रश्न_आयडी": 61, "प्रश्न": "'कर नाही त्याला डर कशाला' म्हणजे काय?", "उत्तर": "ज्याने काही गुन्हा केला नाही, त्याला भीती बाळगण्याचे कारण नाही." },
    { "प्रश्न_आयडी": 62, "प्रश्न": "'नावडतीचे मीठ अळणी' या म्हणीचा अर्थ काय?", "उत्तर": "एखादी व्यक्ती आवडत नसेल, तर तिने केलेले चांगले कामही वाईट वाटते." },
    { "प्रश्न_आयडी": 63, "प्रश्न": "पाठात आलेली दोन नैसर्गिक तत्वे कोणती?", "उत्तर": "पाठात नदी, वारा आणि सूर्य ही नैसर्गिक तत्वे आली आहेत." },
    { "प्रश्न_आयडी": 64, "प्रश्न": "प्रतीकचा आळस कसा गेला?", "उत्तर": "निसर्गाची ऊर्जा पाहून प्रतीकचा आळस पळाला." },
    { "प्रश्न_आयडी": 65, "प्रश्न": "कुंभाराच्या घरात काय होते?", "उत्तर": "कुंभाराच्या घरात माती, चाक आणि मातीची भांडी होती." },
    { "प्रश्न_आयडी": 66, "प्रश्न": "कवितेने कोणाचे मन प्रसन्न होते?", "उत्तर": "कवितेने वाचणाऱ्याचे आणि ऐकणाऱ्याचे मन प्रसन्न होते." },
    { "प्रश्न_आयडी": 67, "प्रश्न": "सात वाजता सूर्य कोठे होता?", "उत्तर": "सात वाजता सूर्य बराच वर आला होता." },
    { "प्रश्न_आयडी": 68, "प्रश्न": "प्रतीक डोळे का चोळत होता?", "उत्तर": "प्रतीक नुकताच झोपेतून उठला होता म्हणून डोळे चोळत होता." },
    { "प्रश्न_आयडी": 69, "प्रश्न": "धड्याचा मुख्य संदेश काय आहे?", "उत्तर": "आयुष्यात सतत चालत राहणे आणि प्रगती करणे हा मुख्य संदेश आहे." },
    { "प्रश्न_आयडी": 70, "प्रश्न": "प्रतीकला कोणाचे हसू ऐकू आले?", "उत्तर": "प्रतीकला नदीचे खळखळून हसणे ऐकू आले." }
  ]
}